{
  "manifest_version": 3,
  "name": "Oliver Less Tabs",
  "description" : "Please stop using this many tabs",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
